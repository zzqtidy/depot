<h1>Store#index</h1>
<% if notice %>
<p id="notice"><%= notice %></p>
<% end  %>
<% @products.each do |product|%>
    <div class="entry">
      <%= image_tag(product.image_url) %>
      <h3><%= product.title %></h3>
      <%= sanitize(product.description) %>
      <div class="store_price">
        <span class="price"><%= product.price %></span>
      </div>
    </div>
<% end %>